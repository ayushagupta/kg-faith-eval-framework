{
    "disease_entity_extraction": {
        "format": {
            "type": "json_schema",
            "name": "disease_entities",
            "strict": true,
            "schema": {
                "type": "object",
                "properties": {
                    "diseases": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": ["diseases"],
                "additionalProperties": false
            }
        }
    },
    "mcq_question": {
        "format": {
            "type": "json_schema",
            "name": "answer",
            "strict": true,
            "schema": {
                "type": "object",
                "properties": {
                    "answer": {
                        "type": "string"
                    }
                },
                "required": ["answer"],
                "additionalProperties": false
            }
        }
    }
}